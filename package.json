{
  "name": "cit-apps-custody",
  "version": "0.0.2",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_host": "ng serve --host 0.0.0.0",
    "external": "ionic cap run android -l --external",
    "build": "ng build",
    "build_android": "ng build && npx cap sync android && npx cap run android",
    "build_android_wifi": "ng build --configuration=production && npx cap sync android && npx cap run android --target=192.168.1.5:5555",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-advance": "ng build && npx capawesome manifests:generate --path www",
    "upload": "npm run build-advance && npx capawesome apps:bundles:create --path www --channel development --appId e2725b51-aba2-462c-b6da-c7521e911c8b",
    "build-advance-upload": "ng build && npx capawesome manifests:generate --path www && npm run upload",
    "lint": "ng lint"
  },
  "private": true,
